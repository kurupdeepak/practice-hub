package com.kurup.loyalityapp.model;

public class User {
	
	private int customerId;
	
	private String name;
	
	public User() {
	}

	public int getCustomerId() {
		return customerId;
	}

	public void setCustomerId(int customerId) {
		this.customerId = customerId;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

}
